{
  "name": "vscode-ai-commit",
  "displayName": "AI Commit++",
  "description": "%description%",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Azure",
    "OpenAI",
    "ChatGPT",
    "GitEmoji",
    "Git Commit",
    "Conventional Commits",
    "Commitizen",
    "Commit Message",
    "Commit Message Generator",
    "AI Commit",
    "Auto Commit"
  ],
  "publisher": "NicholasHsiang",
  "repository": "https://github.com/xianghongai/vscode-ai-commit",
  "icon": "images/logo.png",
  "activationEvents": [
    "onCommand:ai-commit"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.ai-commit",
        "icon": {
          "dark": "images/icon.svg",
          "light": "images/icon-gray.svg"
        },
        "title": "%command.aiCommit%"
      },
      {
        "command": "ai-commit.switchProvider",
        "title": "%command.switchProvider%"
      }
    ],
    "configuration": {
      "properties": {
        "ai-commit.AI_COMMIT_LANGUAGE": {
          "default": "English",
          "description": "%config.language.description%",
          "enum": [
            "Simplified Chinese",
            "Traditional Chinese",
            "Japanese",
            "Korean",
            "Czech",
            "German",
            "French",
            "Italian",
            "Dutch",
            "Portuguese",
            "Vietnamese",
            "English",
            "Spanish",
            "Swedish",
            "Russian",
            "Bahasa",
            "Polish",
            "Turkish",
            "Thai"
          ],
          "enumDescriptions": [
            "简体中文",
            "繁體中文",
            "にほんご",
            "한국어",
            "česky",
            "Deutsch",
            "française",
            "italiano",
            "Nederlands",
            "português",
            "tiếng Việt",
            "english",
            "español",
            "Svenska",
            "русский",
            "bahasa",
            "Polski",
            "Turkish",
            "ไทย"
          ],
          "type": "string"
        },
        "ai-commit.providers": {
          "type": "array",
          "description": "%config.providers.description%",
          "default": [],
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "displayName": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "enum": [
                  "openai",
                  "openai-compatible",
                  "openrouter",
                  "gemini",
                  "azure-openai",
                  "ollama"
                ]
              },
              "model": {
                "type": "string"
              },
              "modelKind": {
                "type": "string",
                "enum": [
                  "chat"
                ]
              },
              "baseUrl": {
                "type": "string"
              },
              "endpoint": {
                "type": "string"
              },
              "deployment": {
                "type": "string"
              },
              "apiKey": {
                "type": "string"
              },
              "apiVersion": {
                "type": "string"
              },
              "organization": {
                "type": "string"
              },
              "headers": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              },
              "timeoutMs": {
                "type": "number"
              },
              "params": {
                "type": "object",
                "properties": {
                  "temperature": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 2
                  },
                  "top_p": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1
                  },
                  "maxTokens": {
                    "type": "number",
                    "minimum": 1
                  }
                },
                "additionalProperties": true
              }
            },
            "required": [
              "id",
              "type",
              "model"
            ]
          }
        },
        "ai-commit.activeProviderId": {
          "type": "string",
          "default": "",
          "description": "%config.activeProviderId.description%"
        },
        "ai-commit.PROMPT_FLAVOR": {
          "type": "string",
          "default": "Conventional Commits",
          "enum": [
            "Conventional Commits",
            "Conventional Commits with Gitmoji"
          ],
          "enumDescriptions": [
            "%config.promptFlavor.conventional%",
            "%config.promptFlavor.conventionalWithGitmoji%"
          ],
          "description": "%config.promptFlavor.description%"
        },
        "ai-commit.PROMPT_FILE": {
          "type": "string",
          "default": "",
          "description": "%config.promptFile.description%"
        }
      },
      "title": "%config.title%"
    },
    "menus": {
      "scm/title": [
        {
          "command": "extension.ai-commit",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "ai-commit.switchProvider",
          "when": "true"
        }
      ]
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "build": "tsup --config tsup.config.ts --minify",
    "compile": "tsup --config tsup.config.ts",
    "compile-tests": "tsc -p . --outDir out",
    "lint": "eslint src",
    "package": "vsce package --no-dependencies",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "publish": "vsce publish --no-dependencies",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run build",
    "watch": "tsup --config tsup.config.ts --watch",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20",
    "@types/vscode": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vscode/test-electron": "^2.3.0",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.37.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "tsup": "^8.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "fs-extra": "^11.0.4",
    "openai": "^4.14.2",
    "simple-git": "^3.17.0"
  },
  "license": "MIT"
}
